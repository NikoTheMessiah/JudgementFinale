local Boinker = Instance.new("BoolValue",Judgement2.GoldYoinker)
local CoinYoink = Instance.new("BoolValue",Judgement2.GoldYoinker)
Judgement2.GoldYoinker.MouseButton1Click:Connect(function()
	if CoinYoink.Value == false and Boinker.Value == false then
		Judgement2.light_2.BackgroundColor3 = Color3.new(0, 1, 0)
		CoinYoink.Value = true
	else
		if CoinYoink.Value == true and Boinker.Value == true then
			Judgement2.light_2.BackgroundColor3 = Color3.new(1, 0, 0)
			CoinYoink.Value = false
		end
	end
end)

local newGradient = Instance.new("UIGradient",COMBOS.SSKnifeRC)
local colorTable = {}
for i = 0,359,20 do
	table.insert(colorTable,ColorSequenceKeypoint.new(i/359,Color3.fromHSV(i/359,1,1)))
end

table.insert(colorTable,ColorSequenceKeypoint.new(1,Color3.fromHSV(1,1,1)))

newGradient.Color = ColorSequence.new(colorTable)

local Gradient1 = newGradient:Clone()
Gradient1.Parent = COMBOS.SSKnifeRCB
local Gradient2 = newGradient:Clone()
Gradient2.Parent = COMBOS.SSKnifeRDTC
local Gradient25 = newGradient:Clone()
Gradient25.Parent = COMBOS.SSKnifeRDTCB
local Gradient4 = newGradient:Clone()
Gradient4.Parent = COMBOS.SSKnifeRNC
local Gradient4 = newGradient:Clone()
Gradient4.Parent = COMBOS.SSKnifeRNCB
local Gradient5 = newGradient:Clone()
Gradient5.Parent = COMBOS.ToughRC
local Gradient6 = newGradient:Clone()
Gradient6.Parent = COMBOS.ToughRCB
local Gradient7 = newGradient:Clone()
Gradient7.Parent = COMBOS.ToughRDTC
local Gradient8 = newGradient:Clone()
Gradient8.Parent = COMBOS.ToughRDTCB
local Gradient9 = newGradient:Clone()
Gradient9.Parent = COMBOS.ToughRNC
local Gradient10 = newGradient:Clone()
Gradient10.Parent =COMBOS.ToughRNCB


CoinYoink:GetPropertyChangedSignal("Value"):Connect(function()
	if CoinYoink.Value == true and Boinker.Value == false then
		Boinker.Value = true
		repeat
			wait()
			game.Workspace:WaitForChild("SansCoin")
			for i,v in pairs(game.Workspace:GetChildren()) do
				if v.Name == "SansCoin" then
					firetouchinterest(game.Players.LocalPlayer.Character.HumanoidRootPart, v.Coin, 0)
					firetouchinterest(game.Players.LocalPlayer.Character.HumanoidRootPart, v.Coin, 1)
				end
			end
		until CoinYoink.Value == false
		Boinker.Value = false
		CoinYoink.Value = false
	else
		Boinker.Value = false
	end
end)

