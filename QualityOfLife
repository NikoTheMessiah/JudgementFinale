local template = PLAYERLISTOPERATION.Person

local scroller = PLAYERLISTOPERATION.PlayerLister 




function updatePlayers()

	for i, child in pairs(scroller:GetChildren()) do

		if child:IsA("Frame") then child:Destroy() end
	end
	for i, plr in pairs(game.Players:GetChildren()) do
		plr:WaitForChild("playerstats")

		local img = game.Players:GetUserThumbnailAsync(plr.UserId, Enum.ThumbnailType.HeadShot, Enum.ThumbnailSize.Size420x420)

		local templateClone = template:Clone()
		templateClone.Parent = scroller
		templateClone.Visible = true
		if plr.playerstats.RESET.Value == 1 then
			templateClone.Player.TextColor3 = Color3.fromRGB(255, 233, 0)
		end
		if plr.leaderstats.Wins.Value >= 10000 then
			templateClone.Player.BackgroundTransparency = 0.7
			templateClone.Player.BackgroundColor3 = Color3.fromRGB(0, 0, 230)
		end
		templateClone.Profile.Image = img
		templateClone.Player.Text = plr.Name..", Wins: "..plr.leaderstats.Wins.Value
		templateClone.Armour.Text = "Armor: "..plr.playerstats.CurrentArmor.Value
		templateClone.Weapon.Text = "Weapon: "..plr.playerstats.CurrentWeapon.Value
		templateClone.Soul.Text = "Soul: "..plr.playerstats.CurrentSoul.Value
		templateClone.Potion.Text = "Equipement: "..plr.playerstats.CurrentPotion.Value
		templateClone.Sans.Text = "Character: "..plr.playerstats.CurrentMonster.Value
		if plr.playerstats.CurrentMonster.Value == "UnderverseNM" then
			templateClone.Sans.Text = "Character: UV!Killer"			
		end
		templateClone.Food.Text = "Pies: "..plr.playerstats.Food.Value



		scroller.CanvasSize = UDim2.new(0, 0, 0, scroller.UIListLayout.AbsoluteContentSize.Y)
	end
end


local player = game.Players.LocalPlayer




CHARACTERS.ERed .IsRED.Value = false
CHARACTERS.E1Geno .IsGENO.Value = false
CHARACTERS.E2Geno .IsGENO.Value = false
CHARACTERS.E3Geno .IsGENO.Value = false
CHARACTERS.E4Geno .IsGENO.Value = false
CHARACTERS.E1Dust.IsDUST.Value = false
CHARACTERS.E2Dust.IsDUST.Value = false
CHARACTERS.E3Dust.IsDUST.Value = false
CHARACTERS.E4Dust.IsDUST.Value = false
CHARACTERS.E5Dust.IsDUST.Value = false
CHARACTERS.E6Dust.IsDUST.Value = false
CHARACTERS.E7Dust.IsDUST.Value = false
CHARACTERS._531HMInsanity .IsInsanity.Value = false
CHARACTERS._53NMD.IsNMD.Value = false
CHARACTERS._52Sudden.IsSudden.Value = false
CHARACTERS._50Font.IsPrankSpare.Value = false
CHARACTERS._49Sansta.IsChristmas.Value = false
CHARACTERS._48Gaster.IsGaster.Value = false
CHARACTERS._54Seraphim.IsRESET.Value = false
CHARACTERS._55DustTrust.IsRESET.Value = false

if game:GetService("MarketplaceService"):UserOwnsGamePassAsync(player.UserId, BADGES_GAMEPASSES.EvolRedID) then
	CHARACTERS.ERed.IsRED.Value = true
else
	CHARACTERS.ERed.IsRED.Value = false
end
if game:GetService("MarketplaceService"):UserOwnsGamePassAsync(player.UserId, BADGES_GAMEPASSES.EvolGenoID) then
	CHARACTERS.E1Geno.IsGENO.Value = true
	CHARACTERS.E2Geno.IsGENO.Value = true
	CHARACTERS.E3Geno.IsGENO.Value = true
	CHARACTERS.E4Geno.IsGENO.Value = true
else
	CHARACTERS.E1Geno.IsGENO.Value = false
	CHARACTERS.E2Geno.IsGENO.Value = false
	CHARACTERS.E3Geno.IsGENO.Value = false
	CHARACTERS.E4Geno.IsGENO.Value = false
end
if game:GetService("MarketplaceService"):UserOwnsGamePassAsync(player.UserId, BADGES_GAMEPASSES.EvolDustID) then
	CHARACTERS.E1Dust.IsDUST.Value = true
	CHARACTERS.E2Dust.IsDUST.Value = true
	CHARACTERS.E3Dust.IsDUST.Value = true
	CHARACTERS.E4Dust.IsDUST.Value = true
	CHARACTERS.E5Dust.IsDUST.Value = true
	CHARACTERS.E6Dust.IsDUST.Value = true
	CHARACTERS.E7Dust.IsDUST.Value = true
else
	CHARACTERS.E1Dust.IsDUST.Value = false
	CHARACTERS.E2Dust.IsDUST.Value = false
	CHARACTERS.E3Dust.IsDUST.Value = false
	CHARACTERS.E4Dust.IsDUST.Value = false
	CHARACTERS.E5Dust.IsDUST.Value = false
	CHARACTERS.E6Dust.IsDUST.Value = false
	CHARACTERS.E7Dust.IsDUST.Value = false
end
if BadgeService:UserHasBadgeAsync(player.UserId, BADGES_GAMEPASSES.InsanityID) then
	CHARACTERS._531HMInsanity .IsInsanity.Value = true
else
	CHARACTERS._531HMInsanity .IsInsanity.Value = false
end
if BadgeService:UserHasBadgeAsync(player.UserId, BADGES_GAMEPASSES.Halloween2021ID) then
	CHARACTERS._532DEATH:FindFirstChild("IsHALLOWEEN2021").Value = true 
else
	CHARACTERS._532DEATH:FindFirstChild("IsHALLOWEEN2021").Value = false
end
if BadgeService:UserHasBadgeAsync(player.UserId, BADGES_GAMEPASSES.NMD_ID) then
	CHARACTERS._53NMD.IsNMD.Value = true
else
	CHARACTERS._53NMD.IsNMD.Value = false
end
if BadgeService:UserHasBadgeAsync(player.UserId, BADGES_GAMEPASSES.SuddenID) then
	CHARACTERS._52Sudden.IsSudden.Value = true
else
	CHARACTERS._52Sudden.IsSudden.Value = false
end
if BadgeService:UserHasBadgeAsync(player.UserId, BADGES_GAMEPASSES.SPAREID) then
	CHARACTERS._50Font.IsPrankSpare.Value = true
else
	CHARACTERS._50Font.IsPrankSpare.Value = false
end
if BadgeService:UserHasBadgeAsync(player.UserId, BADGES_GAMEPASSES.Christmas2020ID) then
	CHARACTERS._49Sansta.IsChristmas.Value = true
else
	CHARACTERS._49Sansta.IsChristmas.Value = false
end
if BadgeService:UserHasBadgeAsync(player.UserId, BADGES_GAMEPASSES.GasterID) then
	CHARACTERS._48Gaster.IsGaster.Value = true
else
	CHARACTERS._48Gaster.IsGaster.Value = false
end
if BadgeService:UserHasBadgeAsync(player.UserId, BADGES_GAMEPASSES.Christmas2021ID) then
	CHARACTERS._533KRAMPUS.IsChristmas2021.Value = true
else
	CHARACTERS._533KRAMPUS.IsChristmas2021.Value = false
end
if BadgeService:UserHasBadgeAsync(player.UserId, BADGES_GAMEPASSES.ResetID) then
	CHARACTERS._54Seraphim.IsRESET.Value = true
	CHARACTERS._55DustTrust.IsRESET.Value = true
else
	CHARACTERS._54Seraphim.IsRESET.Value = false
	CHARACTERS._55DustTrust.IsRESET.Value = false
end
function updateWINS()
	for i,v in pairs(Judgement2._5CharactersFrame:GetChildren()) do
		if v.ClassName == "ImageLabel" then
			if player.leaderstats.Wins.Value >= v.Req.Value then
				v.BackgroundColor3 = Color3.fromRGB(0,255,0)
				v.BackgroundTransparency = 0.8
				if v:FindFirstChild("IsRESET") then
					if v.IsRESET.Value == false then
						v.BackgroundTransparency = 0.85
						v.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
					end
				end
				if v:FindFirstChild("IsGaster") then
					if v.IsGaster.Value == false then
						v.BackgroundTransparency = 0.85
						v.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
					end
				end
				if v:FindFirstChild("IsChristmas") then
					if v.IsChristmas.Value == false then
						v.BackgroundTransparency = 0.85
						v.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
					end
				end
				if v:FindFirstChild("IsPrankSpare") then
					if v.IsPrankSpare.Value == false then
						v.BackgroundTransparency = 0.85
						v.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
					end
				end
				if v:FindFirstChild("IsSudden") then
					if v.IsSudden.Value == false then
						v.BackgroundTransparency = 0.85
						v.BackgroundColor3 = Color3.fromRGB(255, 0, 0)

					end
				end
				if v:FindFirstChild("IsNMD") then
					if v.IsNMD.Value == false then
						v.BackgroundTransparency = 0.85
						v.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
					end
				end
				if v:FindFirstChild("IsHALLOWEEN2021") then
					if v.IsHALLOWEEN2021.Value == false then
						v.BackgroundTransparency = 0.85
						v.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
					end
				end	
				if v:FindFirstChild("IsInsanity") then
					if v.IsInsanity.Value == false then
						v.BackgroundTransparency = 0.85
						v.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
					end
				end
				if v:FindFirstChild("IsChristmas2021") then
					if v.IsHALLOWEEN2021.Value == false then
						v.BackgroundTransparency = 0.85
						v.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
					end
				end	
			else
				v.BackgroundTransparency = 0.85
				v.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
			end
			if v:FindFirstChild("IsDUST") then
				if v.IsDUST.Value == true then
					if player.playerstats.DustEXP.Value >= v.Req.Value then
						v.BackgroundColor3 = Color3.fromRGB(0,255,0)
						v.BackgroundTransparency = 0.8
					else
						v.BackgroundColor3 = Color3.fromRGB(255,0,0)
						v.BackgroundTransparency = 0.8
					end
				else
					v.BackgroundColor3 = Color3.fromRGB(255,0,0)
					v.BackgroundTransparency = 0.8
				end
			end
			if v:FindFirstChild("IsGENO") then
				if v.IsGENO.Value == true then
					if player.playerstats.GenoEXP.Value >= v.Req.Value then
						v.BackgroundColor3 = Color3.fromRGB(0,255,0)
						v.BackgroundTransparency = 0.8
					else
						v.BackgroundColor3 = Color3.fromRGB(255,0,0)
						v.BackgroundTransparency = 0.8
					end
				else
					v.BackgroundColor3 = Color3.fromRGB(255,0,0)
					v.BackgroundTransparency = 0.8
				end
			end
			if v:FindFirstChild("IsRED") then
				if v.IsRED.Value == true then
					if player.playerstats.RedTraining.Value >= v.Req.Value then
						v.BackgroundColor3 = Color3.fromRGB(0,255,0)
						v.BackgroundTransparency = 0.8
					else
						v.BackgroundColor3 = Color3.fromRGB(255,0,0)
						v.BackgroundTransparency = 0.8
					end
				else
					v.BackgroundColor3 = Color3.fromRGB(255,0,0)
					v.BackgroundTransparency = 0.8
				end
			end
		end
	end
end


updatePlayers()
updateWINS()
game.Players.PlayerAdded:Connect(updatePlayers)
game.Players.PlayerRemoving:Connect(updatePlayers)
player.leaderstats.Wins.Changed:Connect(updateWINS)
while true do 
	wait(1)
	updatePlayers()
	updateWINS()
end
