Judgement2.Weapons.MouseButton1Click:Connect(function()
	Judgement2._1WeaponsFrame.Visible = true
	Judgement2._2ArmourFrame.Visible = false
	Judgement2._3SoulFrame.Visible = false
	Judgement2._4CombosFrame.Visible = false
	Judgement2._5CharactersFrame.Visible = false
	_6OVERWRITE.Visible = false
	Judgement2._7INFOFrame.Visible = false
	Judgement2._8OthersFrame.Visible = false
end)

