local player = game.Players.LocalPlayer


player:GetMouse().KeyDown:Connect(function(key)

	if key == "q" and Judgement2.Main.Visible == false then
		Judgement2.Main.Visible = true
	else
		if key == "q" and Judgement2.Main.Visible == true then
			Judgement2.Main.Visible = false
		end
	end
end)
--OVERWRITESTUFF
XOWButton.MouseButton1Click:Connect(function()
	if player.leaderstats.Wins.Value >= 4000 then
		WARNING.Visible = false
		TheOverwriteMenu.Visible = true
	else

	end
end)
KR.MouseButton1Click:Connect(function()
	local args = {
		[1] = "Null"
	}
	game:GetService("Players").LocalPlayer.PlayerGui.OverwriteGUI.Frame.OVERWRITE:FireServer(unpack(args))
end)
