local debounceBrother = false
WEAPONS.ForgettableB.MouseButton1Click:Connect(function()
	if debounceBrother == false then
		debounceBrother = true
		WEAPONS.ForgettableB.Text = "Not out in game. Exists in files."
		wait(1)
		WEAPONS.ForgettableB.Text = "Phantom Papyrus"
		wait(0.5)
		debounceBrother = false
	end
end)
local debounceHandMan = false
WEAPONS.WhatDoYouTwoThinkB.MouseButton1Click:Connect(function()
	if debounceHandMan == false then
		debounceHandMan = true
		WEAPONS.WhatDoYouTwoThinkB.Text = "Not out in game. Exists in files."
		wait(1)
		WEAPONS.WhatDoYouTwoThinkB.Text = "Gaster"
		wait(0.5)
		debounceHandMan = false
	end
end)

