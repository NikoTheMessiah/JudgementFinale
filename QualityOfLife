local debounceLastBreathJacket = false
ARMOUR.LBJB.MouseButton1Click:Connect(function()
	if debounceLastBreathJacket == false then
		debounceLastBreathJacket = true
		ARMOUR.LBJB.Text = "Not out in game. Exists in files."
		wait(1)
		ARMOUR.LBJB.Text = "Last Breath Jacket"
		debounceLastBreathJacket = false
	end
end)
local debounceDisbeliefJacket = false
ARMOUR.DJB.MouseButton1Click:Connect(function()
	if debounceDisbeliefJacket == false then
		debounceDisbeliefJacket = true
		ARMOUR.DJB.Text = "Not out in game. Exists in files."
		wait(1)
		ARMOUR.DJB.Text = "Disbelief Jacket"
		debounceDisbeliefJacket = false
	end
end)

--Combos!

COMBOS.PatienceCB.MouseButton1Click:Connect(function()
	local args = {
		[1] = "Patience"
	}

	game:GetService("ReplicatedStorage").Remotes.ChangeSoul:FireServer(unpack(args))
	local args = {
		[1] = "ToyKnife"
	}

	game:GetService("ReplicatedStorage").Remotes.ChangePlayerWeapon:FireServer(unpack(args))
	local args = {
		[1] = "Ribbon"
	}

	game:GetService("ReplicatedStorage").Remotes.ChangePlayerArmor:FireServer(unpack(args))
end)
COMBOS.JusticeCB.MouseButton1Click:Connect(function()
	local args = {
		[1] = "Justice"
	}

	game:GetService("ReplicatedStorage").Remotes.ChangeSoul:FireServer(unpack(args))
	local args = {
		[1] = "EmptyGun"
	}

	game:GetService("ReplicatedStorage").Remotes.ChangePlayerWeapon:FireServer(unpack(args))
	local args = {
		[1] = "CowboyHat"
	}

	game:GetService("ReplicatedStorage").Remotes.ChangePlayerArmor:FireServer(unpack(args))
end)
