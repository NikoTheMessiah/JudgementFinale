local player = game.Players.LocalPlayer
CLICKCHECK.MouseButton1Click:Connect(function()
	if player.leaderstats.Wins.Value >= 4000 then
		CHECK:Destroy()
		MINIWRITE.Visible = true
	end
end)

player:GetMouse().KeyDown:Connect(function(key)

	if key == "e" and SmallOVERWRITE.Enabled == false then
		SmallOVERWRITE.Enabled = true
	else
		if key == "e" and SmallOVERWRITE.Enabled == true then
			SmallOVERWRITE.Enabled = false
		end
	end
end)

HEAL.MouseButton1Click:Connect(function()
	local args = {
		[1] = "Heal"
	}
	game:GetService("Players").LocalPlayer.PlayerGui.OverwriteGUI.Frame.OVERWRITE:FireServer(unpack(args))
end)
KR.MouseButton1Click:Connect(function()
	local args = {
		[1] = "Null"
	}
	game:GetService("Players").LocalPlayer.PlayerGui.OverwriteGUI.Frame.OVERWRITE:FireServer(unpack(args))
end)
DT.MouseButton1Click:Connect(function()
